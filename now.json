{
    "name": "gin-test",
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build",
            "config": {
              "distDir": "build"
            } 
        }
    ],
    "routes": [{ "src": "/(.*)", "dest": "/src/index.js" }],
    "env": {
        "REACT_APP_PUBLIC_API_KEY": "@reactapppublicapikey",
        "REACT_APP_DEVELOPMENT_LINK": "@reactappdevelopmentlink",
        "REACT_APP_PRODUCTION_LINK": "@reactappproductionlink"
    }
}